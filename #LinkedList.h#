#ifndef LinkedList
#define LinkedList
template <class T>
class LinkedList
{
private: struct NODE
	{
		T info; 
		NODE *next;
	};
		 NODE *LL;
public: LinkedList()
	{
		LL = NULL;
	}
	template<class T>
	void Push (T x)
	{
		NODE *p = new (NODE);
		p -> info = x;
		p -> next = LL;
		LL = p;
	}
	template<class T>
	T Pop()
	{
		NODE *p = LL;
		T x = p -> info;
		OLL = p -> next;
		delete (p);
		return x;
	}
	
	void next()
	{
		LL = LL -> next;
	}
	bool Empty(){return (LL == NULL)?true:false;}
};
#endif
